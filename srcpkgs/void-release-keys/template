maintainer="nox"
pkgname="void-release-keys"
version=1
revision=6
short_desc="Void Linux Release Keys"
homepage="http://www.voidlinux.org"
license="Public Domain"

do_install() {
  for _i in "$FILESDIR"/* ; do
    vinstall "$_i" 644 usr/share/void-release-keys
  done
}

